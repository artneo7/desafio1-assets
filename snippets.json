	"a11.1": {
		"prefix": "a11.1",
		"body": [
			"import { calculadora } from '$$lib/stores';",
			"function resetar() {",
			"  $$calculadora.a = '';",
			"  $$calculadora.b = '';",
			"  $$calculadora.c = '';",
			"  $$calculadora.x = '';",
			"",
			"  // destravar calculadora",
			"  $$calculadora.travar = false;",
			"}",
			""
		],
		"description": "a11.1"
	},
	"a11.2": {
		"prefix": "a11.2",
		"body": [
			"on:click={resetar} on:keypress={resetar}"
		],
		"description": "a11.2"
	},
	"a13.1": {
