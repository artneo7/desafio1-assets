	"a15.1": {
		"prefix": "a15.1",
		"body": [
			"// enviar para o localStorage",
			"localStorage.setItem('resultados', JSON.stringify($$resultados));"
		],
		"description": "a15.1"
	},
	"a15.2": {
		"prefix": "a15.2",
		"body": [
			"import { onMount } from 'svelte';",
			"import { resultados } from '$$lib/stores';",
			"",
			"onMount(() => {",
			"  $$resultados = JSON.parse(localStorage.getItem('resultados')) || [];",
			"})",
			""
		],
		"description": "a15.2"
	},
	"a15.3": {
		"prefix": "a15.3",
		"body": [
