{
	"a2.1": {
		"prefix": "a2.1",
		"body": [
			":root {",
			"  --fonte-primaria: 'Inter', sans-serif;",
			"  --principal: #E9F0EC;",
			"  --bg: #121212;",
			"  --bg-2: #303030;",
			"  --destaque: #39B5E0;",
			"}"
		],
		"description": "a2.1"
	},
	"a2.2": {
		"prefix": "a2.2",
		"body": [
			"/* CSS reset */",
			"*, *::before, *::after {",
			"  box-sizing: border-box;",
			"}",
			"* {",
			"  margin: 0;",
			"}",
			"html {",
			"  scroll-behavior: smooth;",
			"  height: 100%;",
			"}",
			"body {",
			"  font-family: 'Inter', sans-serif;",
			"  line-height: 1.5;",
			"  -webkit-font-smoothing: antialiased;",
			"  color: var(--principal);",
			"  background: var(--bg);",
			"  height: 100%;",
			"}",
			"img, picture, video, canvas, svg {",
			"  display: block;",
			"  max-width: 100%;",
			"}",
			"input, button, textarea, select {",
			"  font: inherit;",
			"}",
			"p, h1, h2, h3, h4, h5, h6 {",
			"  font-size: 1rem;",
			"  overflow-wrap: break-word;",
			"  font-weight: normal;",
			"}",
			"ul {",
			"  list-style-type: none;",
			"  padding: 0;",
			"}",
			"ol {",
			"  padding: 0;",
			"}",
			"a {",
			"  color: var(--principal);",
			"  text-decoration: none;",
			"}",
			"button {",
			"  color: var(--principal);",
			"  border: none;",
			"  cursor: pointer;",
			"  padding: 0;",
			"  background: none;",
			"  touch-action: manipulation;",
			"}",
			"",
			"/* Classes */",
			".container {",
			"  margin: auto;",
			"  padding-left: 40px;",
			"  padding-right: 40px;",
			"}",
			"",
			"@media(max-width: 1000px) {",
			"  .container {",
			"    padding-left: 20px;",
			"    padding-right: 20px;",
			"  }",
			"}"
		],
		"description": "a2.2"
	},
	"a2.3": {
		"prefix": "a2.3",
		"body": [
			"import '$lib/global.css';",
			"import '@fontsource/inter';"
		],
		"description": "a2.3"
	},
	"a3": {
		"prefix": "a3",
		"body": [
			"<section>",
			"  <div class=\"linha linha__1\">",
			"    <input name=\"a\" type=\"number\" placeholder=\"a\" />",
			"    <input name=\"b\" type=\"number\" placeholder=\"b\" />",
			"  </div>",
			"  <div class=\"linha linha__2\">",
			"    <input name=\"c\" type=\"number\" placeholder=\"c\" />",
			"    <input name=\"x\" type=\"number\" placeholder=\"x\" disabled />",
			"  </div>",
			"</section>",
			"",
			"<style>",
			"  section {",
			"    background-color: var(--bg-2);",
			"    padding: 24px;",
			"    display: flex;",
			"    flex-direction: column;",
			"    gap: 8px;",
			"    margin-top: 8px;",
			"    width: fit-content;",
			"  }",
			"  .linha {",
			"    display: flex;",
			"    gap: 40px;",
			"    position: relative;",
			"  }",
			"  .linha::after {",
			"    content: '';",
			"    width: 40px;",
			"    height: 8px;",
			"    background-color: var(--bg);",
			"    position: absolute;",
			"    top: 26px;",
			"    right: 120px;",
			"  }",
			"  input {",
			"    width: 120px;",
			"    height: 60px;",
			"    padding: 16px;",
			"    color: var(--principal);",
			"    font-size: calc(18 / 16 * 1rem);",
			"    background-color: var(--bg);",
			"    border: none;",
			"    text-align: end;",
			"  }",
			"  input::placeholder {",
			"    color: var(--principal);",
			"    text-transform: uppercase;",
			"    opacity: .5;",
			"  }",
			"  input:focus-visible {",
			"    outline: 1px solid var(--principal);",
			"    outline-offset: -1px;",
			"  }",
			"  /* Remover setas type=\"number\" */",
			"  /* https://www.w3schools.com/howto/howto_css_hide_arrow_number.asp */",
			"  input::-webkit-outer-spin-button,",
			"  input::-webkit-inner-spin-button {",
			"    -webkit-appearance: none;",
			"    margin: 0;",
			"  }",
			"  input[type=number] {",
			"    -moz-appearance: textfield;",
			"    appearance: textfield;",
			"  }",
			"</style>"
		],
		"description": "a3"
	},
	"a4.1": {
		"prefix": "a4.1",
		"body": [
			"import { writable } from \"svelte/store\";",
			"",
			"export const calculadora = writable({",
			"  a: '',",
			"  b: '',",
			"  c: '',",
			"  x: ''",
			"});"
		],
		"description": "a4.1"
	},
	"a4.2": {
		"prefix": "a4.2",
		"body": [
			"<script>",
			"  import { calculadora } from '$$lib/stores';",
			"</script>"
		],
		"description": "a4.2"
	},
	"a5.1": {
		"prefix": "a5.1",
		"body": [
			"if (!$$calculadora.a || !$$calculadora.b || !$$calculadora.c) return;"
		],
		"description": "a5.1"
	},
	"a5.2": {
		"prefix": "a5.2",
		"body": [
			"$$calculadora.x = ($$calculadora.b * $$calculadora.c) / $$calculadora.a;"
		],
		"description": "a5.2"
	},
	"a7": {
		"prefix": "a7",
		"body": [
			"<script>",
			"  import { calculadora } from '$lib/stores';",
			"</script>",
			"",
			"<section>",
			"  <p>",
			"    {#if $calculadora.a}",
			"    <span>{$calculadora.a}</span>",
			"    {/if}",
			" ",
			"    {#if $calculadora.b}",
			"    <span>está para {$calculadora.b}.</span>",
			"    {/if}",
			"  </p>",
			"",
			"  <p>",
			"    {#if $calculadora.c}",
			"    <span>Assim como {$calculadora.c}</span>",
			"    {/if}",
			" ",
			"    {#if $calculadora.x}",
			"    <span>está para {$calculadora.x}.</span>",
			"    {/if}",
			"  </p>",
			"</section>",
			"",
			"<style>",
			"  section {",
			"    margin-top: 8px;",
			"    margin-left: 24px;",
			"  }",
			"</style>"
		],
		"description": "a7"
	},
	"a8.1": {
		"prefix": "a8.1",
		"body": [
			"<section>",
			"  <ul class=\"resultados\">",
			"    <li>5 está para 7, assim como 2 está para 2.8</li>",
			"    <li>20 está para 100, assim como 8 está para 40</li>",
			"  </ul>",
			"</section>",
			"",
			"<style>",
			"  section {",
			"    display: grid;",
			"    max-width: 940px;",
			"    margin-top: 32px;",
			"  }",
			"  .resultados {",
			"    border: 1px solid var(--bg-2);",
			"    max-height: 402px;",
			"    overflow-y: auto;",
			"  }",
			"  li {",
			"    font-size: calc(14 / 16 * 1rem);",
			"    height: 40px;",
			"    background-color: var(--bg);",
			"    display: flex;",
			"    align-items: center;",
			"    padding: 16px;",
			"    white-space: nowrap;",
			"  }",
			"  li:first-child {",
			"    margin-top: -1px;",
			"  }",
			"  li + li {",
			"    border-top: 1px solid var(--bg-2);",
			"  }",
			"</style>"
		],
		"description": "a8.1"
	},
	"a8.2": {
		"prefix": "a8.2",
		"body": [
			"// estruturar resultado",
			"let resultado = `${$$calculadora.a} está para ${$$calculadora.b}, assim como ${$$calculadora.c} está para ${$$calculadora.x}`;",
			"",
			"// adicionar resultado ao store",
			"$$resultados = [...$$resultados, resultado];"
		],
		"description": "a8.2"
	},
	"a9": {
		"prefix": "a9",
		"body": [
			"// travar calculadora até o reset",
			"$$calculadora.travar = true;"
		],
		"description": "a9"
	},
